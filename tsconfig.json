{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/*": ["src/app/*"],
      "@img/*": ["src/static/assets/img/*"],
      "@stylesheet/*": ["src/static/assets/stylesheet/*"],
      "@animations/*": ["src/core/animations/*"],
      "@classes/*": ["src/core/classes/*"],
      "@const/*": ["src/core/const/*"],
      "@interfaces*": ["src/core/interfaces/interfaces.ts"],
      "@environment/*": ["src/environments/*"]
    }
  }
}
